{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "eslint:recommended", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "no-console": "off",
    "semi": "off",
    "import/newline-after-import": "off",
    "import/extensions": [{ "js": "always" }]
  }
}
